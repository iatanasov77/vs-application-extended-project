{
    "name": "vankosoft/application-extended-project",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "homepage": "http://vankosoft.org",
    "description": "Vankosoft Extended Project",
    "authors": [
        {
            "name": "Ivan I. Atanasov"
        }
    ],
    "keywords": [
        "symfony",
        "vankosoft",
        "application",
        "catalog",
        "products",
        "services",
        "pricing-plans",
        "shopping-cart",
        "api",
        "api-platform"
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-zmq": "*",
        
        "symfony/dotenv": "7.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/notifier": "7.4.*",
        "symfony/runtime": "7.4.*",
        
        "symfony/http-client": "7.4.*",
        "guzzlehttp/promises": "^2.0",
        "php-http/httplug": "^2.4",
        
        "vankosoft/application": "1.13.*",
        "vankosoft/application-themes": "^1.6",
        "vankosoft/catalog-bundle": "^2.5",
        "vankosoft/borica-bundle": "^0.2",
        "vankosoft/vendo-sdk-bundle": "^0.1",
        "vankosoft/paysera-bundle": "^0.1",
        "vankosoft/api-bundle": "^3.0",
        
        "doctrine/dbal": "^3.0",
        
        "vankosoft/thruway-bundle": "^1.0",
        "cboden/ratchet": "^0.4",
        "react/react": "^1.4",
        "react/partial": "^3.0",
        "react/zmq": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.4.*",
        "symfony/css-selector": "7.4.*",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "7.4.*",
        "symfony/var-dumper": "7.4.*",
        "symfony/web-profiler-bundle": "7.4.*"
    },
    "config": {
         "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "symfony/flex": true,
            "oomphinc/composer-installers-extender": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%/admin-panel": "symfony-cmd",
            "liip:imagine:cache:remove": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    }
}
